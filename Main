def n_w3(event):
    window = Toplevel(root)
    window.geometry('750x300')
    window.config(bg="#40e3e1")

    excel_data_file = xlrd.open_workbook('./Оценки за 1 год/Рассписание.xlsx')
    sheet = excel_data_file.sheet_by_index(0)

    batch_Names_wanted = []
    batch_Names_wanted1 = []

    row_number = sheet.nrows

    if row_number > 0:
         for row in range(0, row_number):
             batch_Names_wanted.append(
                 str(sheet.row(row)[0]).replace("text:", "").replace("'", "").replace(":", "").replace(".", "").replace(
                     "number", "").replace("0", ""))
             batch_Names_wanted1.append(
                 str(sheet.row(row)[1]).replace("text:", "").replace("'", "").replace(".", "").replace(
                     "number", ""))


    tab_control = ttk.Notebook(window)
    tab1 = ttk.Frame(tab_control)
    tab_control.add(tab1, text='Номер урока, Начало-Конец')

    lbl1 = Label(tab1, text='\n'.join(batch_Names_wanted))
    lbl1.grid(column=1, row=0)
    lbl2 = Label(tab1, text='\n'.join(batch_Names_wanted1))
    lbl2.grid(column=2, row=0)
    tab_control.pack(expand=1, fill='both')

def n_w4(event):
    window = Toplevel(root)
    window.geometry('750x300')
    window.config(bg="#40e3e0")
    window.title('Январьские оценки')

    excel_data_file = xlrd.open_workbook('./Оценки за 1 год/Оценки.xlsx')
    sheet = excel_data_file.sheet_by_index(0)

    batch_Names_wanted = []
    batch_Names_wanted1 = []
    batch_Names_wanted2 = []
    batch_Names_wanted3 = []
    batch_Names_wanted4 = []
    batch_Names_wanted5 = []
    batch_Names_wanted6 = []
    batch_Names_wanted7 = []
    batch_Names_wanted8 = []

    row_number = sheet.nrows

    if row_number > 0:
        for row in range(0, row_number):
            batch_Names_wanted.append(
                str(sheet.row(row)[0]).replace("text:", "").replace("'", "").replace(":", "").replace(".",
                                                                                                      "").replace(
                    "number", "").replace("0", "").replace("empty", ""))
            batch_Names_wanted1.append(
                str(sheet.row(row)[1]).replace("empty", "").replace("text:", "").replace("'", "").replace(".",
                                                                                                          "").replace(
                    "number", "").replace(":", "").replace("0", ""))
            batch_Names_wanted2.append(
                str(sheet.row(row)[2]).replace("empty", "").replace("text:", "").replace("'", "").replace(".",
                                                                                                          "").replace(
                    "number", "").replace(":", "").replace("0", ""))
            batch_Names_wanted3.append(
                str(sheet.row(row)[3]).replace("empty", "").replace("text:", "").replace("'", "").replace(".",
                                                                                                          "").replace(
                    "number", "").replace(":", "").replace("0", ""))
            batch_Names_wanted4.append(
                str(sheet.row(row)[4]).replace("empty", "").replace("text:", "").replace("'", "").replace(".",
                                                                                                          "").replace(
                    "number", "").replace(":", "").replace("0", ""))
            batch_Names_wanted5.append(
                str(sheet.row(row)[5]).replace("empty", "").replace("text:", "").replace("'", "").replace(".",
                                                                                                          "").replace(
                    "number", "").replace(":", "").replace("0", ""))
            batch_Names_wanted6.append(
                str(sheet.row(row)[6]).replace("empty", "").replace("text:", "").replace("'", "").replace(".",
                                                                                                          "").replace(
                    "number", "").replace(":", "").replace("0", ""))
            batch_Names_wanted7.append(
                str(sheet.row(row)[7]).replace("empty", "").replace("text:", "").replace("'", "").replace(".",
                                                                                                          "").replace(
                    "number", "").replace(":", "").replace("0", ""))
            batch_Names_wanted8.append(
                str(sheet.row(row)[9]).replace("empty", "").replace("text:", "").replace("'", "").replace(".",
                                                                                                          "").replace(
                    "number", "").replace(":", "").replace("0", ""))

            # batch_Names_wanted9.append(
            # str(sheet.row(row)[9]).replace("empty", "").replace("text:", "").replace("'", "").replace(".",
            # "").replace(
            # "number", "").replace(":", ""))

    tab_control = ttk.Notebook(window)
    tab1 = ttk.Frame(tab_control)
    tab_control.add(tab1, text='ID,ИФ, Оценка')

    lbl1 = Label(tab1, text='\n'.join(batch_Names_wanted))
    lbl1.place(x=0, y=0)
    lbl2 = Label(tab1, text='\n'.join(batch_Names_wanted1))
    lbl2.place(x=60, y=0)
    lbl3 = Label(tab1, text='\n'.join(batch_Names_wanted2))
    lbl3.place(x=90, y=0)
    lbl4 = Label(tab1, text='\n'.join(batch_Names_wanted3))
    lbl4.place(x=285, y=0)
    lbl5 = Label(tab1, text='\n'.join(batch_Names_wanted4))
    lbl5.place(x=315, y=0)
    lbl6 = Label(tab1, text='\n'.join(batch_Names_wanted5))
    lbl6.place(x=575, y=0)
    lbl7 = Label(tab1, text='\n'.join(batch_Names_wanted6))
    lbl7.place(x=365, y=0)
    lbl8 = Label(tab1, text='\n'.join(batch_Names_wanted7))
    lbl8.place(x=465, y=0)
    lbl9 = Label(tab1, text='\n'.join(batch_Names_wanted8))
    lbl9.place(x=520, y=0)


    tab_control.pack(expand=1, fill='both')

