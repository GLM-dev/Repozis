from tkinter import *
from tkinter import ttk
import xlrd

root = Tk()
root.geometry('750x300')
root.config(bg="#40e3e0")
root.title('Система учёта посещаемости учебного заведения')

def n_w1(event):
    window = Toplevel(root)
    window.geometry('580x40')
    window.config(bg="#40e3e0")

    btn1 = Button(window, text="Январь")
    btn1.place(x=20, y=10, width=60, height=20)

    btn2 = Button(window, text="Февраль")
    btn2.place(x=80, y=10, width=60, height=20)

    btn3 = Button(window, text="Март")
    btn3.place(x=140, y=10, width=60, height=20)

    btn4 = Button(window, text="Апрель")
    btn4.place(x=200, y=10, width=60, height=20)

    btn5 = Button(window, text="Май")
    btn5.place(x=260, y=10, width=60, height=20)

    btn6 = Button(window, text="Сентябрь")
    btn6.place(x=320, y=10, width=60, height=20)

    btn7 = Button(window, text="Октябрь")
    btn7.place(x=380, y=10, width=60, height=20)

    btn8 = Button(window, text="Ноябрь")
    btn8.place(x=440, y=10, width=60, height=20)

    btn9 = Button(window, text="Декабрь")
    btn9.place(x=500, y=10, width=60, height=20)

    btn1.bind("<Button-1>", n_w4)
    btn2.bind("<Button-1>", n_w5)
    btn3.bind("<Button-1>", n_w6)
    btn4.bind("<Button-1>", n_w7)
    btn5.bind("<Button-1>", n_w8)
    btn6.bind("<Button-1>", n_w9)
    btn7.bind("<Button-1>", n_w10)
    btn8.bind("<Button-1>", n_w11)
    btn9.bind("<Button-1>", n_w12)





